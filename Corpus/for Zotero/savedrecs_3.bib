@article{ WOS:000272075600003,
Author = {Patriotta, Gerardo and Spedale, Simona},
Title = {Making Sense Through Face: Identity and Social Interaction in a
   Consultancy Task Force},
Journal = {ORGANIZATION STUDIES},
Year = {2009},
Volume = {30},
Number = {11},
Pages = {1227-1248},
Month = {NOV},
Abstract = {In this article we investigate the dynamic connection between individual
   and social processes of sensemaking in the context of group-based
   interaction. Drawing on Goffman's theory of face-to-face behaviour, we
   develop two main arguments. First, the grounding of identity underlying
   group-based interaction typically involves repeated face games during
   which participants attempt to influence the patterns of interaction
   while maintaining a coherent image of self. Second, face games generate
   an `interaction order' that has structuring properties and is therefore
   central to the social construction of sense within a group setting. We
   illustrate our contribution through an empirical study of face games and
   sensemaking within a consultancy task force. The study shows that the
   copresence of participants during group based interaction is in itself
   an occasion for sensemaking as it enacts language-based controversies
   that require composition through shared construction of meaning. In
   addition, our findings highlight that early impressions generate sticky
   patterns of interaction that constrain further exchanges and affect the
   development and outcomes of group sensemaking. p main implication is
   that positive outcomes of sensemaking are contingent upon the ability of
   the participants collectively to generate interaction orders that are
   conducive to working consensus. In this regard, sensegiving mechanisms
   such as leadership can constructively orient interaction amongs
   professionals by providing a common set of expectations about
   behaviours.},
