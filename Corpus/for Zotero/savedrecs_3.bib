@article{ WOS:000245294600004,
Author = {Heath, Joseph},
Title = {An adversarial ethic for business: Or when Sun-Tzu met the stakeholder},
Journal = {JOURNAL OF BUSINESS ETHICS},
Year = {2007},
Volume = {72},
Number = {4},
Pages = {359-374},
Month = {JUN},
Abstract = {In the economic literature on the firm, especially in the
   transaction-cost tradition, a sharp distinction is drawn between
   so-called ``market transactions{''} and ``administered transactions.{''}
   This distinction is of enormous importance for business ethics, since
   market transactions are governed by the competitive logic of the market,
   whereas administered transactions are subject to the cooperative norms
   that govern collective action in a bureaucracy. The widespread failure
   to distinguish between these two types of transactions, and thus to
   distinguish between adversarial and non-adversarial relations, has led
   many business ethicists to develop a ``uniform{''} moral code. Yet in
   market transactions, the checks and balances built into the system of
   commercial exchange are such as to permit more instrumental forms of
   behavior. In administered transactions, by contrast, these checks and
   balances are absent, and thus the institutional context calls for much
   greater exercise of moral restraint. In this paper, I begin the task of
   developing an adversarial ethic for business. According to this view,
   the competitive environment licenses a greater range of
   ``self-interested{''} behavior, but also imposes its own constraints on
   the strategies that firms may adopt in the pursuit of their interests.},
