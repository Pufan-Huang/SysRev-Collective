@article{ WOS:000273301800008,
Author = {Ring, J. Kirk and Peredo, Ana Maria and Chrisman, James J.},
Title = {Business Networks and Economic Development in Rural Communities in the
   United States},
Journal = {ENTREPRENEURSHIP THEORY AND PRACTICE},
Year = {2010},
Volume = {34},
Number = {1},
Pages = {171-195},
Month = {JAN},
Abstract = {Many rural communities in the United States are economically depressed.
   However, while poor rural communities are geographically isolated and
   small, they are likely to be rich in social capital. We first argue that
   the nature of the social capital in such communities can either
   facilitate or constrain the development of business networks among
   entrepreneurs. We then explain the community-level conditions that might
   increase the probability of business network effectiveness. The primary
   opportunities that such networks can exploit and their potential
   spillover effects on economic development are also identified. Finally,
   we provide directions for research that can contribute to better public
   policy decisions.},
