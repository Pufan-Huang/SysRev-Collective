@article{ WOS:000223134000005,
Author = {Colquitt, JA},
Title = {Does the justice of the one interact with the justice of the many?
   Reactions to procedural justice in teams},
Journal = {JOURNAL OF APPLIED PSYCHOLOGY},
Year = {2004},
Volume = {89},
Number = {4},
Pages = {633-646},
Month = {AUG},
Note = {60th Annual Meeting of the Academy-of-Management, TORONTO, CANADA, AUG
   07-09, 2000},
Organization = {Acad Management},
Abstract = {This article reported the results of 2 studies that examined reactions
   to procedural justice in teams. Both studies predicted that individual
   members' reactions would be driven not just by their own procedural
   justice levels but also by the justice experienced by other team
   members. Study I examined intact student teams, whereas Study 2 occurred
   in a laboratory setting. The results showed that individual members' own
   justice interacted with others' justice, such that higher levels of role
   performance occurred when justice was consistent within the team. These
   effects were strongest in highly interdependent teams and weakest for
   members who were benevolent with respect to equity sensitivity.},
