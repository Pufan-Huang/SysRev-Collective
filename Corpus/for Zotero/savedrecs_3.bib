@article{ WOS:000079557200001,
Author = {Lindbeck, A and Nyberg, S and Weibull, JW},
Title = {Social norms and economic incentives in the welfare state},
Journal = {QUARTERLY JOURNAL OF ECONOMICS},
Year = {1999},
Volume = {114},
Number = {1},
Pages = {1-35},
Month = {FEB},
Abstract = {This paper analyzes the interplay between social norms and economic
   incentives in the context of work decisions in the modern welfare state.
   We assume that to live off one's own work is a social norm, and that the
   larger the population share adhering to this norm, the more intensely it
   is felt by the individual. Individuals face two choices: one economic,
   whether to work or Live off public transfers; and one political, hear
   large the transfer should be. The size of the transfer and the intensity
   of the social norm are determined endogenously in equilibrium.},
