@article{ WOS:000180081400004,
Author = {Schaubroeck, J and Lam, SSK},
Title = {How similarity to peers and supervisor influences organizational
   advancement in different cultures},
Journal = {ACADEMY OF MANAGEMENT JOURNAL},
Year = {2002},
Volume = {45},
Number = {6},
Pages = {1120-1136},
Month = {DEC},
Abstract = {This study tested hypotheses concerning how similarity of personality
   traits between promotion candidates and their peers and supervisors
   influences promotion decisions in different work unit cultures.
   Personality similarity to peers was positively associated with promotion
   in units with high individualism. In units with high collectivism,
   supervisor-subordinate personality similarity was instead a significant
   predictor of advancement. Behavioral integration between candidates and
   their peers and supervisors partially mediated the influence of
   personality similarity on promotion. Demographic similarity had little
   influence on promotion decisions. We discuss implications for
   understanding similarity effects and for increasing the validity of
   promotion decisions.},
