@article{ WOS:000269194700006,
Author = {Merz, Michael A. and He, Yi and Vargo, Stephen L.},
Title = {The evolving brand logic: a service-dominant logic perspective},
Journal = {JOURNAL OF THE ACADEMY OF MARKETING SCIENCE},
