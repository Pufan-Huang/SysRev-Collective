@article{ WOS:000265590600003,
Author = {Drago, Francesco and Galbiati, Roberto and Vertova, Pietro},
Title = {The Deterrent Effects of Prison: Evidence from a Natural Experiment},
Journal = {JOURNAL OF POLITICAL ECONOMY},
Year = {2009},
Volume = {117},
Number = {2},
Pages = {257-280},
Month = {APR},
Abstract = {The Collective Clemency Bill passed by the Italian Parliament in July
   2006 represents a natural experiment to analyze the behavioral response
