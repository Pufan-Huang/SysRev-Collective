@article{ WOS:000243324000002,
Author = {Tasa, Kevin and Taggar, Simon and Seijts, Gerard H.},
Title = {The development of collective efficacy in teams: A multilevel and
   longitudinal perspective},
Journal = {JOURNAL OF APPLIED PSYCHOLOGY},
Year = {2007},
Volume = {92},
Number = {1},
Pages = {17-27},
Month = {JAN},
Abstract = {The authors developed and tested a longitudinal multilevel model of
   collective efficacy formation. In 50 self-managing student teams, they
   investigated the effects of individual-level and team-level factors on
   observed behaviors and the subsequent development of collective efficacy
   for mastering a complex team task. Self-efficacy for teamwork,
   task-relevant knowledge, and collective efficacy predicted individual
   teamwork behaviors (rated by peers). Aggregated measures of teamwork
   behavior were related to subsequent collective efficacy, which was
   significantly related to final team performance.},
Publisher = {AMER PSYCHOLOGICAL ASSOC},
Address = {750 FIRST ST NE, WASHINGTON, DC 20002-4242 USA},
