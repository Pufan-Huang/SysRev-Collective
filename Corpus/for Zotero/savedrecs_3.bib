@article{ WOS:000175903700007,
Author = {Bens, DA and Nagar, V and Wong, MHF},
Title = {Real investment implications of employee stock option exercises},
Journal = {JOURNAL OF ACCOUNTING RESEARCH},
Year = {2002},
Volume = {40},
Number = {2},
Pages = {359-393},
Month = {MAY},
Abstract = {This paper examines a real cost of awarding employee stock options.
   Based on the observation that managers are extremely concerned about
   earnings-per-share dilution in equity related compensation, we predict
   and find that firms experiencing significant employee stock option (ESO)
   exercises shift resources away from real investments towards the
   repurchase of their own stocks. We further find weak evidence of a
   decline in subsequent firm performance (as measured by return on assets)
   for several years following the cut in discretionary invesments as a
   result of stock option exercises, though this result is sensitive to the
   metric used to measure performance. Collectively, our findings indicate
   that ESO exercises potentially impose a real cost on the firth in terms
   of foregone investment opportunities.},
