@article{ WOS:000260761000007,
Author = {Rysman, Marc and Simcoe, Timothy},
Title = {Patents and the Performance of Voluntary Standard-Setting Organizations},
Journal = {MANAGEMENT SCIENCE},
Year = {2008},
Volume = {54},
Number = {11},
Pages = {1920-1934},
Month = {NOV},
Abstract = {Voluntary standard-setting organizations (SSOs) are a common feature of
   systems industries, where firms supply interoperable components for a
   shared technology platform. These institutions promote coordinated
   innovation by providing a forum for collective decision making and a
   potential solution to the problem of fragmented and overlapping
   intellectual property rights. This paper examines the economic and
   technological significance of SSOs by analyzing the flow of citations to
   a sample of U. S. patents disclosed during the standard-setting process.
   Our main results show that the age distribution of SSO patent citations
   is shifted toward later years (relative to an average patent) and that
   citations increase substantially following standardization. These
   results suggest that SSOs identify promising technologies and influence
   their subsequent adoption.},
