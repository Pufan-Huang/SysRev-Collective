@article{ WOS:000224279600005,
Author = {Fulk, J and Heino, R and Flanagin, AJ and Monge, PR and Bar, F},
Title = {A test of the individual action model for organizational information
   commons},
Journal = {ORGANIZATION SCIENCE},
Year = {2004},
Volume = {15},
Number = {5},
Pages = {569-585},
Month = {SEP-OCT},
Abstract = {This research elaborated and empirically tested the individual action
   component of the collective action model as applied to individual
   contributions to organizational information commons. The model extended
   prior theory and research by making six elaborations on the classic
   collective action model based on unique characteristics of information
   goods compared to material collective goods. The structural equation
   model was tested via LISREL analyses of data provided by 781 respondents
   in three high-tech firms who had access to corporate intranets as shared
   information goods. The results were highly similar across organizations
   and indicated that (a) level of production, information retrieval, and
   cost predicted the perceived value of information, (b) information value
   and cost predicted gain, and (c) information retrieval and gain
   predicted the level of individual contributions to the commons.},
