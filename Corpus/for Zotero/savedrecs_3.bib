@article{ WOS:000079705300002,
Author = {Gibson, CB},
Title = {Do they do what they believe they can? Group efficacy and group
   effectiveness across tasks and cultures},
Journal = {ACADEMY OF MANAGEMENT JOURNAL},
Year = {1999},
Volume = {42},
Number = {2},
Pages = {138-152},
Month = {APR},
Abstract = {It is argued that the effects of group efficacy (a group's belief
   regarding its ability to perform effectively) are complex and moderated
   by several contingency factors. Findings from two intercultural studies
   support the contingency approach. When task uncertainty was high, team
   members worked independently, and collectivism was low, group efficacy
   was not related to group effectiveness. In contrast, when task
   uncertainty was low, team members worked interdependently, and
   collectivism was high, the relationship between group efficacy and group
   effectiveness was positive. Implications for group-level social
   cognitive theory are discussed.},
