@article{ WOS:000235470100007,
Author = {Nelson, MR and Brunel, FF and Supphellen, M and Manchanda, RV},
Title = {Effects of culture, gender, and moral obligations on responses to
   charity advertising across masculine and feminine cultures},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2006},
Volume = {16},
Number = {1},
Pages = {45-56},
Abstract = {Two studies investigated the effects of charity advertising on
   perceptions of moral obligation to help others and gauged ad evaluation.
   This was done in cultures that were similar in individualism but
   differed in either masculinity (United States and Canada) or femininity
   (Denmark and Norway). Participants read appeals that solicited donations
   to charity by focusing on either egoistic or altruistic motives. In
   masculine cultures, men preferred the egoistic ad and women preferred
   the altruistic one. In feminine cultures, where women are agentic and
   men are allowed to be nurturing, the opposite was the case. Exposure to
   both types of ads activated a sense of personal obligation among men in
   feminine nations and women in masculine nations. However, their
   opposite-sex counterparts reacted against these ads. Implications of
   these findings for an understanding of culture and sex differences in
   advertising effectiveness are discussed.},
