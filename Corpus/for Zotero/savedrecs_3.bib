Language = {English},
Affiliation = {Van der Vegt, GS (Corresponding Author), Univ Groningen, Dept Human Resource Management \& Org Behav, NL-9700 AV Groningen, Netherlands.
   Van der Vegt, Gerben S.; de Jong, Simon B.; Molleman, Eric, Univ Groningen, Dept Human Resource Management \& Org Behav, NL-9700 AV Groningen, Netherlands.
   Bunderson, J. Stuart, Washington Univ, John M Olin Sch Business, St Louis, MO 63130 USA.},
DOI = {10.1287/orsc.1090.0452},
ISSN = {1047-7039},
Keywords = {power asymmetry; learning; teams; performance feedback; task dependence;
