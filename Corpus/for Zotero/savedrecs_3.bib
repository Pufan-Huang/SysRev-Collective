@article{ WOS:000072909200003,
Author = {Lee, F},
Title = {When the going gets tough, do the tough ask for help? Help seeking and
   power motivation in organizations},
Journal = {ORGANIZATIONAL BEHAVIOR AND HUMAN DECISION PROCESSES},
Year = {1997},
Volume = {72},
Number = {3},
Pages = {336-363},
Month = {DEC},
Abstract = {Individuals do not seek help, even when help is needed and available,
   because help seeking implies incompetence and dependence, and therefore
