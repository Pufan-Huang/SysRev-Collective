@article{ WOS:000243182200012,
Author = {Felin, Teppo and Hesterly, William S.},
Title = {The knowledge-based view, nested heterogeneity, and new value creation:
   Philosophical considerations on the locus of knowledge},
Journal = {ACADEMY OF MANAGEMENT REVIEW},
Year = {2007},
Volume = {32},
Number = {1},
Pages = {195-218},
Month = {JAN},
Abstract = {At what level is new value created, or, put differently, what is the
   locus of knowledge? While knowledge and capabilities-based researchers
   argue that the locus of new value and knowledge lies at the firm level,
   we challenge this conceptualization and theoretically build toward more
   individualist foundations. We explicate the underlying philosophical
   assumptions of extant knowledge and capabilities-based work and discuss
   attributional problems. Nested (individual-level. a priori)
   heterogeneity may provide a better explanation of collective
