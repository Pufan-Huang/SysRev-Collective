@article{ WOS:000256364700007,
Author = {Chernykh, Lucy},
Title = {Ultimate ownership and control in Russia},
Journal = {JOURNAL OF FINANCIAL ECONOMICS},
Year = {2008},
Volume = {88},
Number = {1},
Pages = {169-192},
Month = {APR},
Abstract = {I investigate ultimate control and ownership patterns in Russian
   publicly traded companies. I show that these companies are controlled
   either by the state or by anonymous private owners. Federal and regional
   governments' control is exercised through extensive use of pyramids.
   Private owners widely exploit legal loopholes that allow them to mask
   their holdings and identities through nominee and foreign offshore
   arrangements. The comparison of formal and informal ownership disclosure
   reveals that the typical anonymous owners are insiders and that in
   virtually all cases the market participants ``know{''} who the real
   owners are. Collectively, the evidence suggests that the legal
   weaknesses in disclosure requirements are important determinants of
   country-specific ownership and control structures. (C) 2008 Elsevier
   B.V. All rights reserved.},
