@article{ WOS:000073159800015,
Author = {Tinsley, C},
Title = {Models of conflict resolution in Japanese, German, and American cultures},
Journal = {JOURNAL OF APPLIED PSYCHOLOGY},
Year = {1998},
Volume = {83},
Number = {2},
Pages = {316-323},
Month = {APR},
Abstract = {The author developed 3 models for resolving conflict from previous
   literature: resolving conflict by (a) deferring to status power, (b)
   applying regulations, and (c) integrating interests. Preference for a
   model is argued to be influenced by culture, which filters information
   and guides members toward a particular model. The normative conflict
