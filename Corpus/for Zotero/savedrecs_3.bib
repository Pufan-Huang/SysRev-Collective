@article{ WOS:A1996UX40500012,
Author = {Barry, B and Bateman, TS},
Title = {A social trap analysis of the management of diversity},
Journal = {ACADEMY OF MANAGEMENT REVIEW},
Year = {1996},
Volume = {21},
Number = {3},
Pages = {757-790},
Month = {JUL},
Note = {1995 Annual Meeting of the
   International-Association-of-Conflict-Management, HELSINGOR, DENMARK,
   JUN 11-14, 1995},
Organization = {Int Assoc Conflict Management},
Abstract = {Social trap theory is the basis for an analysis of how management
   initiatives influence diversity-related outcomes within work
   organizations. Social traps are situations where decision problems
   involve conflicts between individual or near-term outcomes and
   collective or long-term consequences. The management of diversity is
   conceptualized in terms of the social traps created by workplace
   diversity issues. A social trap model is proposed that (a) identifies
