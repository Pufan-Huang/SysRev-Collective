@article{ WOS:000273570600010,
Author = {Demuijnck, Geert},
Title = {From an Implicit Christian Corporate Culture to a Structured Conception
   of Corporate Ethical Responsibility in a Retail Company: A Case-Study in
   Hermeneutic Ethics},
