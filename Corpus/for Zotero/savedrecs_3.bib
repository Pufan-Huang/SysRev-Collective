@article{ WOS:000079705300006,
Author = {Jung, DI and Avolio, BJ},
Title = {Effects of leadership style and followers' cultural orientation on
   performance in group and individual task conditions},
Journal = {ACADEMY OF MANAGEMENT JOURNAL},
Year = {1999},
Volume = {42},
Number = {2},
Pages = {208-218},
Month = {APR},
Abstract = {We manipulated transformational and transactional leadership styles and
   compared them in individual and group task conditions to determine
   whether they had different impacts on individualists and collectivists
   performing a brainstorming task. Results showed that collectivists with
   a transformational leader generated more ideas, but individualists
   generated more ideas with a transactional leader. Group performance was
   generally higher than that of individuals working alone. However,
   contrary to expectations, collectivists generated more ideas that
   required fundamental organizational changes when working alone.},
