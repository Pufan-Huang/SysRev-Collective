@article{ WOS:000089960800012,
Author = {Mosakowski, E and Earley, PC},
Title = {A selective review of time assumptions in strategy research},
Journal = {ACADEMY OF MANAGEMENT REVIEW},
Year = {2000},
Volume = {25},
Number = {4},
Pages = {796-812},
Month = {OCT},
Abstract = {Concepts of time vary dramatically across individuals and cultures. We
   draw from work in anthropology, psychology, sociology, and management to
   identify five time dimensions that guide our review and discussion of
   dynamic strategic management research. Although strategy researchers
   incorporate time in many ways, they generally ignore a subjective view
   of time and the temporal perceptions of actors in their models. We
   conclude by suggesting how strategy researchers and practitioners can
   incorporate an unambiguous and multifaceted view of time explicitly into
   their work.},
