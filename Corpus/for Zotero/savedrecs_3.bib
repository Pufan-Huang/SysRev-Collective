@article{ WOS:A1996UZ91500002,
Author = {Mintz, SM},
Title = {Aristotelian virtue and business ethics education},
Journal = {JOURNAL OF BUSINESS ETHICS},
Year = {1996},
Volume = {15},
Number = {8},
Pages = {827-838},
Month = {AUG},
Abstract = {In recent years there has been an increased interest in the application
   of Aristotelian virtue to business ethics. The objective of this paper
   is to describe the moral and intellectual virtues defined by Aristotle
   and the types of pedagogy that might be used to integrate virtue ethics
   into the business curriculum. Virtues are acquired human qualities, the
   excellences of character, which enable a person to achieve the good
   life. In business, the virtues facilitate successful cooperation and
   enable the community to achieve its collective goals. The cultivation of
   virtue in students requires imparting knowledge about virtue and
   training students to be virtuous. A variety of instructional techniques
   are discussed including using case studies, collaborative and
   cooperative learning, role-playing, and video presentations. Business
   educators should emphasize to students that virtue considerations apply
   both to possible actions they may take and to themselves as moral
   agents. Since faculty may be viewed as role models, it is especially
   important that they set proper standards of behavior for students to
   emulate.},
