@article{ WOS:000346883300001,
Author = {Suddaby, Roy and Bruton, Garry D. and Si, Steven X.},
Title = {Entrepreneurship through a qualitative lens: Insights on the
   construction and/or discovery of entrepreneurial opportunity},
Journal = {JOURNAL OF BUSINESS VENTURING},
Year = {2015},
Volume = {30},
Number = {1, SI},
Pages = {1-10},
Month = {JAN},
Abstract = {This article applies inductive analytic techniques to identify and
   elaborate on two recurring themes that underpin the core puzzle of
   entrepreneurship research where entrepreneurial opportunities come from.
   The first theme is the unique role of imprinting, or the profound
   influence of social and historical context in constraining the
   perceptual apparatus of entrepreneurs and delimiting the range of
   opportunities for innovation available to them. Second, our analysis
   offers insight into the counterbalancing role of reflexivity, operating
   at both individual and collective levels of analysis, in generating the
   ability of entrepreneurs to overcome the constraints of imprinting.
   These insights are based on a thematic review of the nine studies that
   comprise this special issue on qualitative research. The nine studies,
   individually and each in their own way, offer key insights into how we
   might better understand the emergence of entrepreneurial opportunity.
