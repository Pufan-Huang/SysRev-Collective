   Manning, Stephan, Univ Massachusetts Boston, Coll Management, Boston, MA USA.
   von Hagen, Oliver, Int Trade Ctr, Geneva, Switzerland.},
DOI = {10.1177/0170840612443629},
ISSN = {0170-8406},
EISSN = {1741-3044},
Keywords = {certification; coffee industry; Fairtrade; meta-standardization; social
   movements; standards markets; sustainability; transnational governance;
   voluntary standards},
Keywords-Plus = {INSTITUTIONAL ENTREPRENEURSHIP; POLITICAL STRATEGY; COLLECTIVE ACTION;
