@article{ WOS:000344971100014,
Author = {Jones, Matthew},
Title = {A MATTER OF LIFE AND DEATH: EXPLORING CONCEPTUALIZATIONS OF
   SOCIOMATERIALITY IN THE CONTEXT OF CRITICAL CARE},
