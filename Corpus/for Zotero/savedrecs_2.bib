@article{ WOS:000383627600012,
Author = {Aragon, Cristina and Narvaiza, Lorea and Altuna, Maite},
Title = {Why and How Does Social Responsibility Differ Among SMEs? A Social
   Capital Systemic Approach},
Journal = {JOURNAL OF BUSINESS ETHICS},
Year = {2016},
Volume = {138},
Number = {2},
Pages = {365-384},
Month = {OCT},
Abstract = {The existing analysis of heterogeneous social responsibility (SR) in
   small and medium enterprises (SMEs) has considered the effects of
   individual factors. However, no holistic analysis has been performed on
   how different factors of heterogeneity interact and how they
   collectively affect SR in SMEs. Here, we propose a new systemic
   approach-employing the social capital concept-with the aim of
   identifying how and why SR is built diversely in SMEs. In particular, we
   focus on a positive and holistic perspective that integrates the factors
   proposed in the literature into the social capital construct. Through a
   case study of four Spanish SMEs, this article proposes a systemic
   approach to the study of heterogeneous SR in SMEs.},
