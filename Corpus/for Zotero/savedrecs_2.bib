@article{ WOS:000389104100015,
Author = {Yip, Jeremy A. and Schweitzer, Maurice E.},
Title = {Mad and misleading: Incidental anger promotes deception},
Journal = {ORGANIZATIONAL BEHAVIOR AND HUMAN DECISION PROCESSES},
Year = {2016},
Volume = {137},
Pages = {207-217},
Month = {NOV},
Abstract = {Emotions influence ethical behavior. Across four studies, we demonstrate
   that incidental anger, anger triggered by an unrelated situation,
   promotes the use of deception. In Study 1, participants who felt
   incidental anger were more likely to deceive their counterpart than
   those who felt neutral emotion. In Study 2, we demonstrate that empathy
   mediates the relationship between anger and deception. In Study 3, we
   contrast anger with another negative-valence emotion, sadness. We find
   that participants who felt incidental anger were more likely to use
   deception than were participants who felt incidental sadness or neutral
   emotion. In Study 4, we show that incentives moderate the relationship
   between anger and deception. Collectively, our work reveals that
   incidental anger promotes unethical behavior because angry people become
   less empathetic when pursuing their self-interest. (C) 2016 Elsevier
   Inc. All rights reserved.},
