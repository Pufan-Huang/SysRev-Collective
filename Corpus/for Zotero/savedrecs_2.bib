@article{ WOS:000426864000011,
Author = {Simola, Sheldene},
Title = {Fostering Collective Growth and Vitality Following Acts of Moral
   Courage: A General System, Relational Psychodynamic Perspective},
Journal = {JOURNAL OF BUSINESS ETHICS},
Year = {2018},
Volume = {148},
Number = {1},
Pages = {169-182},
Month = {MAR},
Abstract = {The purpose of this article is to explore a critical paradox related to
   the expression of moral courage in organizations, which is that although
   morally courageous acts are aimed at fostering collective growth,
   vitality, and virtue, their initial result is typically one of
   collective unease, preoccupation, or lapse, reflected in the social
   ostracism and censure of the courageous member and message. Therefore,
   this article addresses the questions of why many organizational groups
   suffer stagnation or decline rather than growth and vitality following
   acts of moral courage, and what can be done to ameliorate this outcome.
   A general system, relational psychodynamic perspective through which
   organizational group members might receive and respond to acts of moral
   courage is offered, and seven insights emerging from this perspective
   for fostering collective growth and vitality following acts of moral
   courage are provided.},
