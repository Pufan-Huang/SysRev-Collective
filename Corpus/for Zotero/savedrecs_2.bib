ISSN = {0167-4544},
EISSN = {1573-0697},
Keywords = {Cross-sector collaboration; Collective action capacity; Beneficiaries;
   Value creation; Systemic change},
Keywords-Plus = {CROSS-SECTOR COLLABORATIONS; PUBLIC VALUE GOVERNANCE; VALUE CREATION;
   RELATIONAL PLURALISM; FRAME ALIGNMENT; RADICAL CHANGE; IMPLEMENTATION;
