@article{ WOS:000317027900002,
Author = {Bingham, Christopher B. and Kahl, Steven J.},
Title = {THE PROCESS OF SCHEMA EMERGENCE: ASSIMILATION, DECONSTRUCTION,
   UNITIZATION AND THE PLURALITY OF ANALOGIES},
Journal = {ACADEMY OF MANAGEMENT JOURNAL},
Year = {2013},
Volume = {56},
Number = {1},
Pages = {14-34},
Month = {FEB},
Abstract = {Schemas are a central concept in strategy and organization theory. Yet,
   despite the importance of schemas, little is known about how they
   emerge. Our in-depth historical analysis of how groups in the life
   insurance industry developed their schema for the computer from
   1945-1975 addresses this gap. We identify three key processes
   assimilation, deconstruction, and unitization-that collectively explain
   and resolve an inherent tension related to schema emergence: how to make
   the unfamiliar familiar but conceptually distinct. We also find that
   each process relates to analogical transfer, but in a more pluralistic
   and dynamic way than the existing literature describes. Broadly, these
   findings have important implications for organizational change and
   managerial cognition.},
