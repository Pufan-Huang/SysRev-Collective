@article{ WOS:000297691100007,
Author = {Cormier, Denis and Lapointe-Antunes, Pascale and Magnan, Michel},
Title = {Revisiting the Relevance and Reliability of Non-GAAP Reporting: The Case
   of the Income Trusts},
Journal = {CONTEMPORARY ACCOUNTING RESEARCH},
Year = {2011},
Volume = {28},
Number = {5},
Pages = {1585+},
Month = {WIN},
Abstract = {The tension between relevance and reliability in measurement is a
   critical concern for securities regulators when firms disclose non-GAAP
   performance measures. It also underlies many aspects of accounting
   standard setting, with standard setters recently giving an edge to
   relevance when developing new standards (e.g., fair value accounting).
   However, prior empirical evidence on industry-specific measures and on
   pro forma earnings does not provide consistent guidance as to how
   investors assess such tension. We investigate a novel unaudited non-GAAP
   performance measure, distributable cash. Distributable cash is the
   primary performance indicator disclosed by income trusts, a flow-through
   entity which became a popular investment vehicle after 2000. It purports
   to represent the amount of cash an income trust is capable of
   distributing to its investors on a sustaining basis. We find that
   distributable cash is deemed to be value relevant by investors, actually
   relegating audited GAAP earnings to a secondary role. However,
   distributable cash's measurement is subject to extensive managerial
   discretion, is unaudited, and is unverifiable by investors. In this
   regard, we document that reported levels of distributable cash exhibit a
   smoothing pattern, thus ensuring stable cash distributions to investors.
   We show also that managerial incentives (stock option holdings) and
   corporate governance (board and ownership) variables affect the level of
   reported distributable cash. Finally, after estimating the discretionary
   and nondiscretionary components of distributable cash, we find that
   their value relevance is similar. We infer from this finding that
   investors, lacking details about underlying measurement assumptions, do
   not seem able to see through managerial opportunism and seem to focus on
   distributable cash as reported. Because the valuation of income trusts,
   collectively worth more than \$200 billion at their peak, relies
   significantly on distributable cash, our results suggest that unaudited
   non-GAAP performance measures can play an important role in capital
   markets. However, our results also indicate such measures need to be
   closely monitored by boards and regulators, thus cautioning accounting
   standard setters against their current emphasis value relevance over
   reliability.},
