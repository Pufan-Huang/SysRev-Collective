@article{ WOS:000394952000004,
Author = {Schneider, Anselm and Wickert, Christopher and Marti, Emilio},
Title = {Reducing Complexity by Creating Complexity: ASystems Theory Perspective
   on How Organizations Respond to Their Environments},
Journal = {JOURNAL OF MANAGEMENT STUDIES},
Year = {2017},
Volume = {54},
Number = {2, SI},
Pages = {182-208},
Month = {MAR},
Abstract = {Organizations have to cope with the complexity of their environment in
   order to survive. A considerable body of research has shown that
   organizations may respond to environmental complexity by creating
   internal complexity - for example, by expanding internal structures and
   processes. However, researchers know less about how organizations create
   collaborative complexity collectively - for example, by establishing
   alliances or developing common standards. This paper uses social systems
   theory to explore how organizations collaborate in response to
   complexity and to analyse the conditions under which they create either
   internal or collaborative complexity (or both) to address environmental
   complexity. Italso examines how these types of complexity feed back into
   environmental complexity. Toillustrate our conceptual model, we use
   corporate social responsibility (CSR).},
