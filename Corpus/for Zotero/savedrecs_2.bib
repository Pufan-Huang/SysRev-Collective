   Pandza, Krsto; Ellwood, Paul, Univ Leeds, Sch Business, Leeds LS2 9JT, W Yorkshire, England.},
DOI = {10.1016/j.respol.2013.02.007},
ISSN = {0048-7333},
EISSN = {1873-7625},
Keywords = {Responsible innovation; Strategy; Virtue ethics; Organizational
