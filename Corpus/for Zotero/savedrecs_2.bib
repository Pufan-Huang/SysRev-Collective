@article{ WOS:000340563700001,
Author = {Cron, William L. and Baldauf, Artur and Leigh, Thomas W. and
   Grossenbacher, Samuel},
Title = {The strategic role of the sales force: perceptions of senior sales
   executives},
Journal = {JOURNAL OF THE ACADEMY OF MARKETING SCIENCE},
Year = {2014},
Volume = {42},
Number = {5},
Pages = {471-489},
Month = {SEP},
Abstract = {Current models of sales force strategy imply formidable information
   processing demands, which leads us to take a cognitive approach to
   studying the issue of sales force strategy. We focus on how top-level
   executives use mental models of sales force performance to simplify the
   issue of sales force strategy. We interviewed 74 senior executives
   responsible for their firms' selling function using the repertory grid
   approach, as this methodology has been shown to be particularly
   effective at uncovering the collective cognitive maps on which
   executives' decisions and behaviors are based. Executives identified a
   broad set of 37 strategic concepts that they felt distinguish the sales
   force efforts of directly competing companies. A second set of sales
   executives classified the 37 concepts into capabilities, resources, and
   organizational context concepts. Based on the classification results and
   feedback from both sets of executives, we developed research
   propositions for examining sales force strategy and provide directions
   for future research.},
