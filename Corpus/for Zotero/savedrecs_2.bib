Author = {Hui, Chun and Lee, Cynthia and Wang, Hui},
Title = {Organizational Inducements and Employee Citizenship Behavior: The
   Mediating Role of Perceived Insider Status and the Moderating Role of
   Collectivism},
Journal = {HUMAN RESOURCE MANAGEMENT},
Year = {2015},
Volume = {54},
Number = {3},
Pages = {439-456},
Month = {MAY-JUN},
Abstract = {The organizational inducement model proposed in this study seeks to
   examine the mediating role of perceived insider status (PIS) and the
   moderating role of vertical collectivism on the relationship between
   organizational inducements and employees' organizational citizenship
   behavior (OCB). Among a sample of Chinese employees, the authors find
   that the organizational inducements of perceived supervisor support and
   participation led to higher levels of PIS, which in turn enhanced OCB.
   Furthermore, vertical collectivism moderated the relationship between
   PIS and OCB, such that their relationship grew stronger when
   collectivism was high. These findings have notable implications for
   theory and practice. (c) 2014 Wiley Periodicals, Inc.},
