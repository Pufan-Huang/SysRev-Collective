@article{ WOS:000300124000020,
Author = {Liu, Wu and Friedman, Ray and Hong, Ying-Yi},
Title = {Culture and accountability in negotiation: Recognizing the importance of
   in-group relations},
Journal = {ORGANIZATIONAL BEHAVIOR AND HUMAN DECISION PROCESSES},
Year = {2012},
Volume = {117},
Number = {1},
Pages = {221-234},
Month = {JAN},
Abstract = {We extend Gelfand and Realo's (1999) argument that accountability
   motivates negotiators from relationally-focused cultures to use a more
