@article{ WOS:000355379100009,
Author = {Johnson, Paul D. and Smith, Mickey B. and Wallace, J. Craig and Hill,
   Aaron D. and Baron, Robert A.},
Title = {A Review of Multilevel Regulatory Focus in Organizations},
Journal = {JOURNAL OF MANAGEMENT},
Year = {2015},
Volume = {41},
Number = {5},
Pages = {1501-1529},
Month = {JUL},
Abstract = {Over the past 15 years, regulatory focus has gained prominence as a
   theory of self-regulatory motivation. Building from personality and
   social psychology research, the nomological network of regulatory focus
   spans individuals, groups, and organizations. This review provides an
   appraisal of regulatory focus from a multilevel perspective as it
   relates to organizational behavior, human resources, strategic
   management, and entrepreneurship. We begin with a discussion of the
   theoretical foundations of regulatory focus, including hierarchical
   motivation and regulatory fit theory. Using these foundations, we
   summarize empirical research on regulatory focus and provide actionable
   avenues for future research on regulatory focus, with particular
   attention paid to how individuals adjust their motivational strategies
   on the basis of context. We also consider regulatory focus as a
   collective function of teams to develop our understanding of
   motivational processes in the workplace.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Review},
Language = {English},
