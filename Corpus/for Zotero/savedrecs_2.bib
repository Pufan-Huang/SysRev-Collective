@article{ WOS:000346883300009,
Author = {Singh, Smita and Corner, Patricia Doyle and Pavlovich, Kathryn},
Title = {Failed, not finished: A narrative approach to understanding venture
   failure stigmatization},
Journal = {JOURNAL OF BUSINESS VENTURING},
Year = {2015},
Volume = {30},
Number = {1, SI},
Pages = {150-166},
Month = {JAN},
Abstract = {This paper implements a qualitative, narrative approach to investigate
   entrepreneurs' personal experience of stigma associated with venture
   failure. Findings draw on the lived experience of 12 entrepreneurs and
   tell a collective story of how stigma affects entrepreneurs, shapes
   their actions, and engenders outcomes for them and their ventures. The
   story covers three episodes of entrepreneurs anticipating, meeting, and
   then transforming venture failure. Overall the paper shifts the focus of
   stigma research from the socio-cultural perspective pervading research
   to date, to micro-level processes underlying socio-cultural trends.
   Findings offer unexpected insights into failure stigmatization. First,
   findings suggest stigmatization is best viewed as a process that unfolds
   over time rather than a label. Second, this process begins before, not
   after, failure and contributes to venture demise. Third, there is a
   positive ending to the collective story in that stigmatization
   ultimately triggers epiphanies or deep personal insights which transform
   entrepreneurs' view of failure from a very negative to a positive life
   experience. This transformation results in entrepreneurs distributing
   learning from failure to the founding of future ventures, even when
   ventures are not their own. (C) 2014 Elsevier Inc. All rights reserved.},
