@article{ WOS:000387881100007,
Author = {Yamazaki, Yoshitaka and Yoon, Jeewhan},
Title = {A Cross-National Study of Fairness in Asia: How Perceptions of a
   Lack-of-Group Bias and Transparency in the Performance Evaluation System
   Relate to Job Satisfaction},
Journal = {HUMAN RESOURCE MANAGEMENT},
Year = {2016},
Volume = {55},
Number = {6},
Pages = {1059-1077},
Month = {NOV-DEC},
Abstract = {This study explored how managers' fairness perceptions of performance
   evaluation systems differ across countries and relate to their job
   satisfaction. Lack-of-group bias and transparency were the constructs
   used to assess fairness perceptions. The data sample consisted of 903
   Asian managers from the subsidiaries of a leading multinational
   corporation (MNC) strategically expanding its retail markets in Japan,
   China, Hong Kong, Malaysia, and Thailand. Results showed that the
   fairness perceptions of lack-of-group bias and transparency concerning
   the common performance evaluation system varied within these Asian
   countries. Furthermore, those fairness perceptions were significantly
   related to job satisfaction among Asian managers overall, as well as in
   each of the five geographical subgroups with the exception of Hong Kong,
   where the perception of a lack-of-group bias was only marginally related
   to job satisfaction. These findings offer theoretical implications
   regarding organizational justice, cross-national management, and
   performance evaluation, as well as practical implications for leveraging
   organizational justice perceptions of performance evaluation systems for
   the effectiveness of MNCs. (c) 2015 Wiley Periodicals, Inc.},
