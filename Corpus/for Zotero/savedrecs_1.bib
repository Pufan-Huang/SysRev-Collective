@article{ WOS:001141717000007,
Author = {Delbridge, Rick and Helfen, Markus and Pekarek, Andreas and Schuessler,
   Elke and Zietsma, Charlene},
Title = {Organizing Sustainably: Introduction to the Special Issue},
