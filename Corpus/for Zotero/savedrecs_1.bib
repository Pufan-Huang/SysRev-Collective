@article{ WOS:000814116600001,
Author = {Enache, Luminita and Li, Lynn and Riedl, Edward J.},
Title = {Regulatory Approval and Biotechnology Product Disclosures},
Journal = {CONTEMPORARY ACCOUNTING RESEARCH},
