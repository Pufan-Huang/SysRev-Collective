@article{ WOS:000525781400010,
Author = {To, Christopher and Leslie, Lisa M. and Torelli, Carlos J. and Stoner,
   Jennifer L.},
Title = {Culture and social hierarchy: Collectivism as a driver of the
   relationship between power and status},
Journal = {ORGANIZATIONAL BEHAVIOR AND HUMAN DECISION PROCESSES},
Year = {2020},
Volume = {157},
Pages = {159-176},
Month = {MAR},
Note = {13th Annual INGRoup Conference, St Louis, MO, JUL 20-22, 2017},
Organization = {INGRoup},
Abstract = {Power and status are distinct bases of social hierarchy with unique
   effects. Yet evidence suggests wide variation in whether perceptions of
   status and power are highly correlated versus relatively distinct. We
   use a cross-cultural lens to explain this variation and suggest cultural
   orientation shapes the effect of power on perceived status and vice
   versa. Six studies using various methodologies and samples demonstrate
   that: (1) individuals high (versus low) on vertical collectivism are
   more likely to perceive high power targets as also high in status; (2)
   individuals high (versus low) on horizontal collectivism are more likely
   to perceive high status targets as also high in power; and (3) cultural
   differences in the power-status relationship qualify prior conclusions
   regarding established effects of power and status on one downstream
   consequence-namely, fairness enacted towards others. Implications for
   theory, practice, and future research are discussed.},
