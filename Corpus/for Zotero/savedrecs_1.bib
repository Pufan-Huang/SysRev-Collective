@article{ WOS:000860825000001,
Author = {Adrian, Christofer and Garg, Mukesh and Anh Viet Pham and Phang,
   Soon-Yeow and Truong, Cameron},
Title = {Do Natural Disasters Affect Corporate Tax Avoidance? The Case of Drought},
Journal = {JOURNAL OF BUSINESS ETHICS},
Year = {2023},
Volume = {186},
Number = {1},
Pages = {105-135},
Month = {AUG},
Abstract = {Natural disaster events such as drought affect the broader economy and
   inflict adverse consequences for firms because of spill-over effects in
   an integrated economy. Contrary to the expectation that firms would
   engage in higher levels of corporate tax avoidance strategies when they
   experience a negative cash flow shock, we document consistent evidence
   that firms engage in less corporate tax avoidance when their headquarter
   states experience drought. Reduced tax avoidance is more pronounced
   among firms with higher CSR performance and among firms operating in
   states that experience a GDP decline. Collectively, the findings of our
   study demonstrate prosocial and ethical behavior of U.S. firms when they
   experience natural disaster events.},
