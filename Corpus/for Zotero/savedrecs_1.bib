   Arnould, Eric, Aalto Univ, Dept Mkt, Ekonominaukio 1, Espoo 02150, Finland.},
DOI = {10.1007/s10551-024-05752-6},
EarlyAccessDate = {JUL 2024},
ISSN = {0167-4544},
