@article{ WOS:000536040500003,
Author = {Bouvard, Matthieu and Lee, Samuel},
Title = {Risk Management Failures},
Journal = {REVIEW OF FINANCIAL STUDIES},
Year = {2020},
Volume = {33},
Number = {6},
Pages = {2468-2505},
Month = {JUN},
Abstract = {We model risk management as information acquisition that delays trading
   decisions. In markets with preemptive competition, this can lead to a
   race to the bottom, where prioritizing trade execution over risk
   management is optimal for each firm, but collectively inefficient. As
   time competition intensifies, mean trading profit supplants risk
   concerns as the main driver of risk management quality, causing risk
   misallocation to rise with trading speed and volume. This pathology of
   risk management failure-the trio of time-consuming risk assessment,
   preemptive competition, and boom markets-has distinctive regulatory
   implications.},
