@article{ WOS:000985593700001,
Author = {Ali, Ashiq and Durney, Michael T. and Fisch, Jill and Kyung, Hoyoun},
Title = {Managers' private communications with analysts: The effect of SEC v.
   Siebel Systems, Inc.},
Journal = {CONTEMPORARY ACCOUNTING RESEARCH},
Year = {2023},
Volume = {40},
Number = {3},
Pages = {1641-1670},
Month = {SEP},
Abstract = {In 2005, the SEC suffered a high-profile loss in its first court case,
   SEC v. Siebel Systems, Inc., in an effort to enforce Regulation Fair
   Disclosure (Reg FD). We examine the impact of this loss on managers'
   selective disclosure to sell-side analysts. We provide evidence that the
   informativeness of analyst reports increased after the Siebel decision,
   especially for observable instances of private meetings. This finding
   suggests that such selective disclosure increased significantly after
   the court's decision. Our results also suggest that the increased
   selective disclosure faded as the SEC resumed enforcement actions
   related to Reg FD in 2009. In exploratory analyses, we survey and
   interview law firm partners to investigate possible mechanisms for our
   results; their responses suggest that the Siebel outcome reduced manager
   concern about liability from selective disclosure. Collectively, our
   results highlight how the anticipated costs of regulatory enforcement
   affect private information flow from managers to analysts in particular.},
