EISSN = {1467-6486},
Keywords = {collective engagement; competitive advantage; competitive intensity;
   customer satisfaction; service quality; shared vision},
Keywords-Plus = {WITHIN-GROUP AGREEMENT; RESOURCE-BASED VIEW; WORK ENGAGEMENT; MARKET
