@article{ WOS:000472587300018,
Author = {Ancona, Deborah and Backman, Elaine and Isaacs, Kate},
Title = {Nimble Leadership Walking the line between creativity and chaos},
Journal = {HARVARD BUSINESS REVIEW},
Year = {2019},
Volume = {97},
Number = {4},
Pages = {74+},
Month = {JUL-AUG},
Abstract = {Nobody really recommends command-and-control leadership anymore. But no
   fully formed alternative has emerged. So mature companies often struggle
   to balance the need for innovation with the need for discipline.
   The authors studied two exceptions: the new-product development stars
   PARC and W.L. Gore. Both companies, they learned, have three distinct
   types of leaders. Entrepreneurial leaders, found at lower levels, create
   new products and services and move their firms into unexplored
   territory. Enabling leaders, in the middle, make sure the entrepreneurs
   have the resources they need. And architecting leaders, near the top,
   monitor culture, high-level strategy, and structure.
   This system allows both companies to be self-managing to a surprising
   degree. Employees choose their work assignments and dream up new
   projects, whose success rests on colleagues' volunteering to join in
   making the companies collective prediction markets. And the mechanisms
   that enable self-management also balance freedom and control: The
   companies function efficiently and exploit new opportunities even as
   they minimize rules.},
