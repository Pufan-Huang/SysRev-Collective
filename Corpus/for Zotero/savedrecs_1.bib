@article{ WOS:000692306400001,
Author = {Nalli, Federica},
Title = {What Mutual Assistance Is, and What It Could Be in the Contemporary
   World},
Journal = {JOURNAL OF BUSINESS ETHICS},
Year = {2023},
Volume = {182},
Number = {4, SI},
Pages = {1041-1053},
Month = {FEB},
Abstract = {This paper explores the implications of a Civil Economy approach to
   consumer ethics, by addressing the idea that Antonio Genovesi's
   (1713-1769) notion of mutual assistance can be understood in terms of
   collective intentionality or team reasoning. I try to give reasons for
   this idea by a careful examination of Genovesi's conception of social
   life and human agency and by reading it through the lens of team
   reasoning. I argue that this understanding of mutual assistance may
   imply broad constraints over agents' choices whenever they conflict with
   the good of society. Then I explore the implications of a mutual
   assistance approach to market ethics in a global society, where
   conflicting views of good and different group affiliations are possible.},
