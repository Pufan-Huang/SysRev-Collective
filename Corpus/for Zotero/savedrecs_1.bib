@article{ WOS:001159909500001,
Author = {Suquet, Jean-Baptiste and Collard, Damien},
Title = {Maintaining ``Good{''} Care: An Articulation Work Perspective on
   Organizational Ethics in the Healthcare Sector},
Journal = {JOURNAL OF BUSINESS ETHICS},
Year = {2024},
Volume = {194},
Number = {3},
Pages = {545-560},
Month = {OCT},
Abstract = {The literature on organizational ethics has paved the way for a situated
   and intersubjective understanding of ethics through caring practices. In
   this article, we try to extend this perspective by looking beyond the
   interactions of caregivers among themselves or with care seekers to
   reveal ethics as the ongoing collective accomplishment of a variety of
   actors. We do so by mobilizing Strauss's theoretical perspective of
   articulation work in the context of healthcare. Based on an ethnography,
   we show how actors of care (e.g., nurses, caregivers, or doctors)
   operate within care arrangements that need to be frequently
   rearticulated to face ethical threats and protect their patients'
   well-being. We distinguish three types of rearticulation corresponding
   to different degrees of rearticulation, individual actor stance, and
   specific ways of working things out. We discuss their implications for
   the maintenance of organizational ethics.},
